{
  "name": "nurlresolver-netlify",
  "version": "2.12.2",
  "description": "nurlresolver host on netlify",
  "main": "functions/server.js",
  "type": "module",
  "scripts": {
    "build": "esbuild express/server.ts --bundle --platform=node --target=ES2022 --outfile=functions/server.js --format=esm",
    "build:serve": "netlify-lambda serve express --config ./webpack.config.js",
    "start": "nodemon server-local.js",
    "start:watch": "tsc-watch --onSuccess \"node server-local.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mnsrulz/nurlresolver-netlify.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mnsrulz/nurlresolver-netlify/issues"
  },
  "homepage": "https://github.com/mnsrulz/nurlresolver-netlify#readme",
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "emitter": "0.0.5",
    "esbuild": "^0.17.15",
    "express": "^4.18.2",
    "netlify-lambda": "^2.0.16",
    "nodemon": "^2.0.22",
    "nurlresolver": "^3.0.3",
    "serverless-http": "^3.2.0",
    "typescript": "^5.0.4"
  }
}
